{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Server",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "es6",
    "noEmit": false,
    "module": "commonjs",
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}